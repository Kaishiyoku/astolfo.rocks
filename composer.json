{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3",
        "doctrine/dbal": "2.11.1",
        "fideloper/proxy": "4.4.0",
        "fruitcake/laravel-cors": "2.0.2",
        "guzzlehttp/guzzle": "7.1.0",
        "laravel/framework": "8.6.0",
        "laravel/helpers": "1.3.0",
        "laravel/tinker": "2.4.2",
        "symfony/css-selector": "5.1.6",
        "symfony/dom-crawler": "5.1.6"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "3.5.1",
        "barryvdh/laravel-ide-helper": "2.8.1",
        "facade/ignition": "2.3.7",
        "fzaninotto/faker": "1.9.1",
        "laravel-frontend-presets/tailwindcss": "^4.3",
        "mockery/mockery": "1.4.2",
        "nunomaduro/collision": "5.0.2",
        "phpunit/phpunit": "^9.3",
        "spatie/phpunit-snapshot-assertions": "4.2.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan ide-helper:models --dir=\"app/Models\" --write"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
