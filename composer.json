{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.4|^8.0",
        "bensampo/laravel-enum": "^3.4",
        "doctrine/dbal": "^3.1.0",
        "fideloper/proxy": "^4.4.1",
        "fruitcake/laravel-cors": "^2.0.4",
        "guzzlehttp/guzzle": "^7.3.0",
        "intervention/image": "^2.5.1",
        "kaishiyoku/laravel-cli-create-user": "^3.1",
        "laravel/framework": "^8.45.1",
        "laravel/helpers": "^1.4.1",
        "laravel/tinker": "^2.6.1",
        "symfony/css-selector": "^5.3.0",
        "symfony/dom-crawler": "^5.3.0",
        "tioga-tours/php-imgfing": "^0.3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6.1",
        "barryvdh/laravel-ide-helper": "^2.10.0",
        "facade/ignition": "^2.10.1",
        "fakerphp/faker": "^1.16",
        "knuckleswtf/scribe": "^3.14",
        "laravel-frontend-presets/tailwindcss": "^4.3.1",
        "laravel/breeze": "^1.4",
        "mockery/mockery": "^1.4.3",
        "nunomaduro/collision": "^5.4.0",
        "phpunit/phpunit": "^9.3",
        "spatie/phpunit-snapshot-assertions": "^4.2.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "ide-helper": [
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan ide-helper:models --dir=\"app/Models\" --write"
        ],
        "api-docs": [
            "php artisan scribe:generate"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
