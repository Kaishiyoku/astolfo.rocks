{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2",
        "doctrine/dbal": "2.10.0",
        "fideloper/proxy": "4.2.1",
        "guzzlehttp/guzzle": "6,4,1",
        "laravel/framework": "5.8.27",
        "laravel/tinker": "1.0.10",
        "laravelcollective/html": "5.8.0",
        "symfony/css-selector": "5.0.0",
        "symfony/dom-crawler": "5.0.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "3.2.8",
        "barryvdh/laravel-ide-helper": "2.6.5",
        "filp/whoops": "2.5.0",
        "fzaninotto/faker": "1.9.0",
        "mockery/mockery": "1.3.0",
        "nunomaduro/collision": "3.0.1",
        "phpunit/phpunit": "8.4.3",
        "spatie/phpunit-snapshot-assertions": "3.0.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan ide-helper:models --dir=\"app/Models\" --write"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
