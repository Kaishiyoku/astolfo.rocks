{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "doctrine/dbal": "2.9.2",
        "fideloper/proxy": "4.1.0",
        "laravel/framework": "5.6.39",
        "laravel/tinker": "1.0.8",
        "laravelcollective/html": "5.6.10",
        "symfony/dom-crawler": "4.2.3",
        "webup/laravel-sendinblue": "1.1.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "3.2.2",
        "barryvdh/laravel-ide-helper": "2.5.3",
        "filp/whoops": "2.3.1",
        "fzaninotto/faker": "1.8.0",
        "mockery/mockery": "1.2.0",
        "nunomaduro/collision": "2.1.1",
        "phpunit/phpunit": "8.0.1"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan ide-helper:models --dir=\"app/Models\" --write"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
